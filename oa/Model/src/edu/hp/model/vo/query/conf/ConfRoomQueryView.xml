  <SQLQuery><![CDATA[SELECT ConfRoomCalendar.ID, 
       ConfRoomCalendar.USER_ID, 
       ConfRoomCalendar.USER_DISPLAY_NAME, 
       ConfRoomCalendar.LOCATION_ID, 
       ConfRoomCalendar.LOCATION_NAME, 
       ConfRoomCalendar.MEETING_ROOM, 
       ConfRoomCalendar.MEETING_ROOM_ID, 
       ConfRoomCalendar.TITLE, 
       ConfRoomCalendar.PARTICIPANTS, 
       ConfRoomCalendar.START_TIME, 
       ConfRoomCalendar.END_TIME, 
       ConfRoomCalendar.COMMENTS, 
       ConfRoomCalendar.NEED_PROJECTOR, 
       ConfRoomCalendar.NEED_LOUDSPEAKER, 
       ConfRoomCalendar.NEED_BEVERAGE, 
       ConfRoomCalendar.NEED_FRUITS, 
       ConfRoomCalendar.NEED_SNACKS, 
       ConfRoomCalendar.SNACK_LEVEL, 
       ConfRoomCalendar.NUM_OF_PEOPLE, 
       ConfRoomCalendar.STATE, 
       ConfRoomCalendar.PR_ID,
ConfRoomCalendar.SUBMIT_DATE,
ConfRoomCalendar.ALLDAY
FROM CONF_ROOM_CALENDAR ConfRoomCalendar
WHERE ConfRoomCalendar.MEETING_ROOM_ID IN
  (
    SELECT
      regexp_substr(:confRmNos,'[^,]+', 1, level)
    FROM
      dual CONNECT BY regexp_substr(:confRmNos, '[^,]+', 1, level) IS NOT NULL
  )
AND ConfRoomCalendar.STATE = :initStat]]></SQLQuery>
