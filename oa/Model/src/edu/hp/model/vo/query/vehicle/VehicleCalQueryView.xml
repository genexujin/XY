       VehicleCalendar.TITLE, 
       VehicleCalendar.CONTACT_ID, 
       VehicleCalendar.CONTACT_NAME, 
       VehicleCalendar.CONTACT_PHONE, 
       VehicleCalendar.STATE, 
       VehicleCalendar.RETURN_TRIP, 
       VehicleCalendar.START_TIME, 
       VehicleCalendar.END_TIME, 
       VehicleCalendar.RETURN_START_TIME, 
       VehicleCalendar.RETURN_END_TIME, 
       VehicleCalendar.TRIP_START, 
       VehicleCalendar.TRIP_DEST, 
       VehicleCalendar.RETURN_TRIP_START, 
       VehicleCalendar.RETURN_TRIP_END, 
       VehicleCalendar.NUM_OF_PEOPLE, 
       VehicleCalendar.COMMENTS, 
       VehicleCalendar.USER_ID,
       VehicleCalendar.SUBMIT_DATE
FROM VEHICLE_CALENDAR VehicleCalendar
WHERE VehicleCalendar.VEHICLE_ID IN
  (
    SELECT
      regexp_substr(:vehicleIds,'[^,]+', 1, level)
    FROM
      dual CONNECT BY regexp_substr(:vehicleIds, '[^,]+', 1, level) IS NOT NULL
  )
AND VehicleCalendar.STATE = '已调度']]></SQLQuery>
