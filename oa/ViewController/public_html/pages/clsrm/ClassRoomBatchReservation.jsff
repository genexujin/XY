                                                           autoSubmit="true" immediate="false">
                                            <af:selectItem label="全天" value="ALLDAY" id="si5"/>
                                            <af:selectItem label="非全天" value="TIME" id="si6"/>
                                        </af:selectOneRadio>
                                    <af:inputNumberSpinbox value="#{bindings.NumOfPeople.inputValue}"
