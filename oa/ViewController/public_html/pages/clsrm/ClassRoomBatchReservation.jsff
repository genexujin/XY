                              queryListener="#{bindings.ClsRmBtchRsvtnViewQuery.processQuery}" filterVisible="false"
                              varStatus="vs"
                              selectedRowKeys="#{bindings.ClsRmBtchRsvtnView.collectionModel.selectedRow}"
