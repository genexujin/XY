    <af:panelSplitter orientation="vertical" splitterPosition="250" id="ps1">
        <f:facet name="first">
            <af:panelHeader text="批量登记汇总" id="ph1" type="stretch">
                <f:facet name="context"/>
                <f:facet name="menuBar"/>
                <f:facet name="toolbar"/>
                <f:facet name="legend"/>
                <f:facet name="info"/>
                <af:panelCollection id="pc1" featuresOff="statusBar">
                    <f:facet name="menus"/>
                    <f:facet name="toolbar">
                        <af:toolbar id="t1">
                            <af:commandToolbarButton
                                                     text="新预订" disabled="#{!bindings.CreateInsert.enabled}"
                                                     id="ctb1" icon="/images/icons/new_ena.png"
                                                     actionListener="#{bindings.CreateInsert.execute}"/>
                            <af:commandToolbarButton text="删除"
                                                     disabled="#{!bindings.Delete.enabled}" id="ctb2"
                                                     icon="/images/icons/delete_ena.png">
                                <af:showPopupBehavior triggerType="click" popupId="p1"/>
                            </af:commandToolbarButton>
                            <af:commandToolbarButton text="导出Excel" id="ctb4"
                                                     icon="/images/icons/export.png">
                                <af:exportCollectionActionListener type="excelHTML" exportedId="t2" title="教室批量预定汇总 "
                                                                   filename="ClassroomReservation.xls"/>
                            </af:commandToolbarButton>
                        </af:toolbar>
                    </f:facet>
                    <f:facet name="statusbar"/>
