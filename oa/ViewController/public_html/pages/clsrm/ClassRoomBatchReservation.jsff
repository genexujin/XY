                                   headerText="#{bindings.ClsRmBtchRsvtnView.hints.ActStartTime.label}" id="c4">
                            <af:outputText value="#{row.ActStartTime}" id="ot4">
                                <af:convertDateTime pattern="#{bindings.ClsRmBtchRsvtnView.hints.ActStartTime.format}"
