                                                            autoSubmit="true" contentStyle="width:150px;">
                                            <f:selectItems value="#{bindings.LocationName.items}" id="si3"/>
                                        </af:selectOneChoice>
                                        <af:selectOneChoice value="#{bindings.Classroom.inputValue}"
