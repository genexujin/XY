                                  label="#{bindings.PurchaseOrderLinesView.hints.ExpectedDate.label}"
                                  required="#{bindings.PurchaseOrderLinesView.hints.ExpectedDate.mandatory}"
                                  columns="#{bindings.PurchaseOrderLinesView.hints.ExpectedDate.displayWidth}"
                                  shortDesc="#{bindings.PurchaseOrderLinesView.hints.ExpectedDate.tooltip}" id="id1"
                                  readOnly="#{pageFlowScope.fromMenu eq 'verifier' or pageFlowScope.fromMenu eq 'approver' or pageFlowScope.fromMenu eq 'receiver' 
                                              or (bindings.State.inputValue ne '1' and bindings.State.inputValue ne '4')}"
                                      minValue="#{pageFlowScope.MyPoBean.currDate}" autoSubmit="true">
                        <!--<f:validator binding="#{row.bindings.ExpectedDate.validator}"/>-->
                        <af:convertDateTime pattern="#{bindings.PurchaseOrderLinesView.hints.ExpectedDate.format}"/>
                    </af:inputDate>
                    </af:column>
                    <!--<af:column sortProperty="#{bindings.PurchaseOrderLinesView.hints.State.name}" sortable="false"
                               headerText="#{bindings.PurchaseOrderLinesView.hints.State.label}" id="c40" width="80">
                        <af:selectOneChoice value="#{row.bindings.State.inputValue}" label="#{row.bindings.State.label}"
                                            required="#{bindings.PurchaseOrderLinesView.hints.State.mandatory}"
                                            shortDesc="#{bindings.PurchaseOrderLinesView.hints.State.tooltip}" id="soc30"
                                            readOnly="#{!sessionScope.LoginUserBean.isUserInRole['采购审核']}"
                                            autoSubmit="true">
                            <f:selectItems value="#{row.bindings.State.items}" id="si30"/>
                        </af:selectOneChoice>
                    </af:column>-->
                    <af:column sortProperty="#{bindings.PurchaseOrderLinesView.hints.Cancelled.name}" sortable="true"
                               headerText="#{bindings.PurchaseOrderLinesView.hints.Cancelled.label}" id="c20">
                          <af:selectBooleanCheckbox value="#{row.bindings.Cancelled.inputValue}"
                                      label="#{row.bindings.Cancelled.label}"
                                      shortDesc="#{bindings.PurchaseOrderLinesView.hints.Cancelled.tooltip}" id="sbc1"
                                      autoSubmit="true"
                                      readOnly="#{!(pageFlowScope.fromMenu eq 'normal' and (bindings.State.inputValue eq '1' or bindings.State.inputValue eq '4'))
                                              and !(pageFlowScope.fromMenu eq 'verifier' and bindings.State.inputValue eq '2')
                                              and !(pageFlowScope.fromMenu eq '-' and bindings.State.inputValue eq '2')}"/>
                    </af:column>
                    <af:column headerText="#{bindings.PurchaseOrderLinesView.hints.SubmitNote.label}"
                               sortProperty="#{bindings.PurchaseOrderLinesView.hints.SubmitNote.name}" sortable="false"
                               id="c9" width="120">
                        <af:inputText value="#{row.bindings.SubmitNote.inputValue}"
                                  label="#{bindings.PurchaseOrderLinesView.hints.SubmitNote.label}"
                                  required="#{bindings.PurchaseOrderLinesView.hints.SubmitNote.mandatory}"
                                  columns="#{bindings.PurchaseOrderLinesView.hints.SubmitNote.displayWidth}"
                                  maximumLength="#{bindings.PurchaseOrderLinesView.hints.SubmitNote.precision}"
                                  shortDesc="#{bindings.PurchaseOrderLinesView.hints.SubmitNote.tooltip}" id="it7"
                                  readOnly="#{pageFlowScope.fromMenu eq 'verifier' or pageFlowScope.fromMenu eq 'approver' or pageFlowScope.fromMenu eq 'receiver' 
                                              or (bindings.State.inputValue ne '1' and bindings.State.inputValue ne '4')}">
                        <f:validator binding="#{row.bindings.SubmitNote.validator}"/>
                    </af:inputText>
                    </af:column>
                    <af:column headerText="#{bindings.PurchaseOrderLinesView.hints.PurchaseQuantity.label}"
                               sortProperty="#{bindings.PurchaseOrderLinesView.hints.PurchaseQuantity.name}"
                               sortable="false" id="c11" width="60">
                               <!--rendered="#{pageFlowScope.fromMenu eq 'verifier' or pageFlowScope.fromMenu eq 'approver' or pageFlowScope.fromMenu eq 'buyer' or pageFlowScope.fromMenu eq 'receiver'
                                           or sessionScope.LoginUserBean.isUserInRole['采购审核'] or sessionScope.LoginUserBean.isUserInRole['采购审批']}"-->
                                          
                        <af:inputText value="#{row.bindings.PurchaseQuantity.inputValue}"
                                  label="#{bindings.PurchaseOrderLinesView.hints.PurchaseQuantity.label}"
                                  required="#{bindings.PurchaseOrderLinesView.hints.PurchaseQuantity.mandatory}"
                                  columns="#{bindings.PurchaseOrderLinesView.hints.PurchaseQuantity.displayWidth}"
                                  maximumLength="#{bindings.PurchaseOrderLinesView.hints.PurchaseQuantity.precision}"
                                  shortDesc="#{bindings.PurchaseOrderLinesView.hints.PurchaseQuantity.tooltip}"
                                  id="it9"                                  
                                  readOnly="#{pageFlowScope.fromMenu eq 'approver' or pageFlowScope.fromMenu eq 'receiver' or bindings.State.inputValue ne '2'
