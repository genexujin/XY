<?xml version='1.0' encoding='UTF-8'?>
<ui:composition xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
                xmlns:f="http://java.sun.com/jsf/core">
    <af:panelStretchLayout id="psl1" startWidth="0px" endWidth="0px" topHeight="250px" bottomHeight="0px">
        <f:facet name="top">
          <af:panelGroupLayout layout="vertical" id="pgl1">
            <af:panelHeader text="订单" id="ph1" >
                    <af:panelFormLayout id="pfl1" rows="5">
                        <af:selectOneChoice value="#{bindings.SubmitterId.inputValue}" label="申请人"
                                            required="#{bindings.SubmitterId.hints.mandatory}"
                                            shortDesc="#{bindings.SubmitterId.hints.tooltip}" id="soc1" autoSubmit="true"
                                            valueChangeListener="#{pageFlowScope.MyPoBean.submitterChanged}" disabled="true">
                            <f:selectItems value="#{bindings.SubmitterId.items}" id="si1"/>
                        </af:selectOneChoice>
                        <af:inputDate value="#{bindings.SubmitAt.inputValue}" id="ot4" label="提交日期" columns="20"
                                      disabled="true">
                        </af:inputDate>
                        <af:selectOneChoice value="#{bindings.ItemCategoryId1.inputValue}"  autoSubmit="true"
                                            label="采购类别" valueChangeListener="#{pageFlowScope.MyPoBean.itemCategoryChanged}"
                                            required="#{bindings.ItemCategoryId1.hints.mandatory}"
                                            shortDesc="#{bindings.ItemCategoryId1.hints.tooltip}" id="soc3"
                                            disabled="#{(pageFlowScope.fromMenu eq 'verifier' or pageFlowScope.fromMenu eq 'approver' or pageFlowScope.fromMenu eq 'receiver') or bindings.State.inputValue ne '1'}">
                            <f:selectItems value="#{bindings.ItemCategoryId1.items}" id="si3"/>
                        </af:selectOneChoice>
                        <af:inputText value="#{bindings.SubmitTotal.inputValue}" id="ot7" label="预算总金额" disabled="true" columns="20"
                                      binding="#{pageFlowScope.MyPoBean.submitTotalComp}">
                            <af:convertNumber type="currency"/>
                        </af:inputText>
                        <af:inputText value="#{bindings.VerifyTotal.inputValue}" id="ot8" label="审核总金额" disabled="true" columns="20">
                            <af:convertNumber type="currency"/>
                        </af:inputText>
                        <af:selectOneChoice value="#{bindings.State.inputValue}" label="订单状态"
                                            required="#{bindings.State.hints.mandatory}"
                                            shortDesc="#{bindings.State.hints.tooltip}" id="soc2" disabled="true">
                            <f:selectItems value="#{bindings.State.items}" id="si2"/>
                        </af:selectOneChoice>
                        <af:inputText value="#{bindings.OrderNote.inputValue}" id="ot9" label="备注" rows="5" columns="25"
                                      disabled="#{(pageFlowScope.fromMenu eq 'verifier' or pageFlowScope.fromMenu eq 'approver' or pageFlowScope.fromMenu eq 'receiver') or bindings.State.inputValue ne '1'}"/>
                        <af:spacer width="5px" height="5px"/>
                        <af:spacer width="5px" height="5px"/>
                        <af:spacer width="5px" height="5px"/>
                        <af:table value="#{bindings.PurchaseOrderHistorysView.collectionModel}" var="row"
                                  rows="#{bindings.PurchaseOrderHistorysView.rangeSize}"
                                  emptyText="#{bindings.PurchaseOrderHistorysView.viewable ? '暂无数据。' : 'Access Denied.'}"
                                  fetchSize="#{bindings.PurchaseOrderHistorysView.rangeSize}" rowBandingInterval="0"
                                  id="t3" autoHeightRows="7" width="400" columnStretching="last"
                                  partialTriggers="::cb2 ::cb3 ::cb4 ::cb40 ::cb5 ::cb50 ::cb6 ::cb7 ::cb8 ::cb18 ::cb19">
                            <af:column sortProperty="#{bindings.PurchaseOrderHistorysView.hints.OperationDate.name}"
                                       sortable="true"
                                       headerText="#{bindings.PurchaseOrderHistorysView.hints.OperationDate.label}"
                                       id="c1" width="150"> 
                                <af:outputText value="#{row.OperationDate}" id="ot1">
                                    <af:convertDateTime pattern="#{bindings.PurchaseOrderHistorysView.hints.OperationDate.format}"/>
                                </af:outputText>
                            </af:column>
                            <af:column sortProperty="#{bindings.PurchaseOrderHistorysView.hints.OperatorName.name}"
                                       sortable="true"
                                       headerText="#{bindings.PurchaseOrderHistorysView.hints.OperatorName.label}"
                                       id="c2" width="60">
                                <af:outputText value="#{row.OperatorName}" id="ot2"/>
                            </af:column>
                            <af:column sortProperty="#{bindings.PurchaseOrderHistorysView.hints.OperationDetail.name}"
                                       sortable="true"
                                       headerText="#{bindings.PurchaseOrderHistorysView.hints.OperationDetail.label}"
                                       id="c10">
                                <af:outputText value="#{row.OperationDetail}" id="ot3"/>
                            </af:column>
                        </af:table>
                        <f:facet name="footer">
                            <af:panelGroupLayout layout="horizontal" id="pgl2">
                                <f:facet name="separator">
                                    <af:spacer width="5" height="1" id="s1"/>
                                </f:facet>
                                <af:commandButton text="保存" partialSubmit="true" id="cb1" actionListener="#{pageFlowScope.MyPoBean.savePo}"
                                                  rendered="#{(pageFlowScope.fromMenu!='approver' and pageFlowScope.fromMenu!='verifier') 
                                                              and (bindings.State.inputValue eq '1' or bindings.State.inputValue eq '4' or bindings.State.inputValue eq '5')}"
                                                  icon="/images/icons/filesave.png"/>
                                <af:commandButton text="返回" partialSubmit="true" id="rb1" actionListener="#{bindings.Rollback.execute}" immediate="true" 
                                                  action="return" rendered="#{pageFlowScope.fromMenu != '-' and pageFlowScope.fromMenu!='create'}"
                                                  icon="/images/icons/rollback.png"/>
                                <af:commandButton text="提交" partialSubmit="true" id="cb2" actionListener="#{pageFlowScope.MyPoBean.submitPo}"
                                                  rendered="#{(pageFlowScope.fromMenu!='approver' and pageFlowScope.fromMenu!='verifier' and pageFlowScope.fromMenu!='receiver')
                                                              and (bindings.State.inputValue eq '1' or bindings.State.inputValue eq '4')}"
                                                  icon="/images/icons/run.png"/>
                                <af:commandButton text="审核" partialSubmit="true" id="cb3" actionListener="#{pageFlowScope.MyPoBean.verifyPo}"
                                                  rendered="#{(pageFlowScope.fromMenu eq 'verifier' or sessionScope.LoginUserBean.isUserInRole['采购审核'])
                                                               and bindings.State.inputValue eq '2'}"
                                                  icon="/images/icons/check.png"/>
                                <af:commandButton text="审批" partialSubmit="true" id="cb4" actionListener="#{pageFlowScope.MyPoBean.approvePo}"
                                                  rendered="#{(pageFlowScope.fromMenu eq 'approver' or sessionScope.LoginUserBean.isUserInRole['采购审批'])
                                                               and bindings.State.inputValue eq '3' and bindings.CurrentApprover.inputValue eq '采购审批'}"
                                                  icon="/images/icons/check.png"/>
                                <af:commandButton text="审批" partialSubmit="true" id="cb40" actionListener="#{pageFlowScope.MyPoBean.approvePo2nd}"
                                                  rendered="#{(pageFlowScope.fromMenu eq 'approver' or sessionScope.LoginUserBean.isUserInRole['采购终审'])
                                                               and bindings.State.inputValue eq '3' and bindings.CurrentApprover.inputValue eq '采购终审'}"
                                                  icon="/images/icons/check.png"/>
                                <af:commandButton text="拒绝" partialSubmit="true" id="cb5" actionListener="#{pageFlowScope.MyPoBean.rejectPo}"
                                                  rendered="#{(pageFlowScope.fromMenu eq 'approver' or sessionScope.LoginUserBean.isUserInRole['采购审批'])
                                                               and bindings.State.inputValue eq '3' and bindings.CurrentApprover.inputValue eq '采购审批'}"
                                                  icon="/images/icons/terminate.png"/>
                                <af:commandButton text="拒绝" partialSubmit="true" id="cb50" actionListener="#{pageFlowScope.MyPoBean.rejectPo}"
                                                  rendered="#{(pageFlowScope.fromMenu eq 'approver' or sessionScope.LoginUserBean.isUserInRole['采购终审'])
                                                               and bindings.State.inputValue eq '3' and bindings.CurrentApprover.inputValue eq '采购终审'}"
                                                  icon="/images/icons/terminate.png"/>
                                <af:commandButton text="取消" partialSubmit="true" id="cb6" actionListener="#{pageFlowScope.MyPoBean.cancelPo}"
                                                  rendered="#{(pageFlowScope.fromMenu eq 'normal' and (bindings.State.inputValue eq '1' or bindings.State.inputValue eq '2' or bindings.State.inputValue eq '4'))
                                                              or (sessionScope.LoginUserBean.isUserInRole['采购审核'] and bindings.State.inputValue ne '1')}"
                                                  icon="/images/icons/undo.png"/>
                                <af:commandButton text="完成" partialSubmit="true" id="cb7" actionListener="#{pageFlowScope.MyPoBean.executePo}"
                                                  rendered="#{(pageFlowScope.fromMenu eq 'buyer' or sessionScope.LoginUserBean.isUserInRole['采购执行'])
                                                              and bindings.State.inputValue eq '5'}"
                                                  icon="/images/icons/check.png"/>
                                <af:commandButton text="收货" partialSubmit="true" id="cb8" actionListener="#{pageFlowScope.MyPoBean.finishPo}"
                                                  rendered="#{(pageFlowScope.fromMenu eq 'receiver' or sessionScope.LoginUserBean.isUserInRole['采购收货'])
                                                              and bindings.State.inputValue eq '5'}"
                                                  icon="/images/icons/package.png"/>
                                <af:commandButton text="重新打开" partialSubmit="true" id="cb18" actionListener="#{pageFlowScope.MyPoBean.reopenPo}"
                                                  rendered="#{(sessionScope.LoginUserBean.isUserInRole['采购审核'] or sessionScope.LoginUserBean.isUserInRole['采购执行'])
                                                              and bindings.State.inputValue eq '6'}"
                                                  icon="/images/icons/open.png"/>
                                <af:commandButton text="重新审核" partialSubmit="true" id="cb19" actionListener="#{pageFlowScope.MyPoBean.backToVerifyPo}"
                                                  rendered="#{sessionScope.LoginUserBean.isUserInRole['采购审核'] 
                                                              and (bindings.State.inputValue ne '1' and bindings.State.inputValue ne '2')}"
                                                  icon="/images/icons/redo.png"/>
                            </af:panelGroupLayout>
                        </f:facet>
                    </af:panelFormLayout>
                
            </af:panelHeader>
            <af:spacer width="50px" height="20px"/>
            <af:panelHeader text="订单行" id="ph2">
            </af:panelHeader>        
          </af:panelGroupLayout>
        </f:facet>
        <f:facet name="center">
          <af:panelCollection id="pc1">
