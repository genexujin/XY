                                  required="#{bindings.PurchaseOrderLinesView.hints.SubmitUnit.mandatory}"
                                  columns="#{bindings.PurchaseOrderLinesView.hints.SubmitUnit.displayWidth}"
                                  maximumLength="#{bindings.PurchaseOrderLinesView.hints.SubmitUnit.precision}"
                                  shortDesc="#{bindings.PurchaseOrderLinesView.hints.SubmitUnit.tooltip}" id="it4"
                                  readOnly="#{pageFlowScope.fromMenu eq 'verifier' or pageFlowScope.fromMenu eq 'approver' or pageFlowScope.fromMenu eq 'receiver' 
                                              or (bindings.State.inputValue ne '1' and bindings.State.inputValue ne '4')}"
                                      autoSubmit="true">
                        <!--<f:validator binding="#{row.bindings.SubmitUnit.validator}"/>-->
                    </af:inputText>
                    </af:column>
                    <af:column headerText="#{bindings.PurchaseOrderLinesView.hints.SubmitPrice.label}"
                               sortProperty="#{bindings.PurchaseOrderLinesView.hints.SubmitPrice.name}" sortable="false"
                               id="c6" width="60">
                        <af:inputText value="#{row.bindings.SubmitPrice.inputValue}"
                                  label="#{bindings.PurchaseOrderLinesView.hints.SubmitPrice.label}"
                                  required="#{bindings.PurchaseOrderLinesView.hints.SubmitPrice.mandatory}"
                                  columns="#{bindings.PurchaseOrderLinesView.hints.SubmitPrice.displayWidth}"
                                  maximumLength="#{bindings.PurchaseOrderLinesView.hints.SubmitPrice.precision}"
                                  shortDesc="#{bindings.PurchaseOrderLinesView.hints.SubmitPrice.tooltip}" id="it5"
                                  readOnly="#{pageFlowScope.fromMenu eq 'approver' or pageFlowScope.fromMenu eq 'receiver' 
                                              or (bindings.State.inputValue ne '2' and bindings.State.inputValue ne '1' and bindings.State.inputValue ne '4')}"
                                      autoSubmit="true">
                        <f:validator binding="#{row.bindings.SubmitPrice.validator}"/>
                    </af:inputText>
