                               id="c14">
                        <af:inputText value="#{row.bindings.VerifyNote.inputValue}"
                                  label="#{bindings.PurchaseOrderLinesView.hints.VerifyNote.label}"
                                  required="#{bindings.PurchaseOrderLinesView.hints.VerifyNote.mandatory}"
                                  columns="#{bindings.PurchaseOrderLinesView.hints.VerifyNote.displayWidth}"
                                  maximumLength="#{bindings.PurchaseOrderLinesView.hints.VerifyNote.precision}"
                                  shortDesc="#{bindings.PurchaseOrderLinesView.hints.VerifyNote.tooltip}" id="it12">
                            <!--<f:validator binding="#{row.bindings.VerifyNote.validator}"/>-->
                        </af:inputText>
                    </af:column>
                    <af:column headerText="#{bindings.PurchaseOrderLinesView.hints.ActualPrice.label}"
                               sortProperty="#{bindings.PurchaseOrderLinesView.hints.ActualPrice.name}" sortable="false"
                               id="c12">
                        <af:inputText value="#{row.bindings.ActualPrice.inputValue}"
                                  label="#{bindings.PurchaseOrderLinesView.hints.ActualPrice.label}"
                                  required="#{bindings.PurchaseOrderLinesView.hints.ActualPrice.mandatory}"
                                  columns="#{bindings.PurchaseOrderLinesView.hints.ActualPrice.displayWidth}"
                                  maximumLength="#{bindings.PurchaseOrderLinesView.hints.ActualPrice.precision}"
                                  shortDesc="#{bindings.PurchaseOrderLinesView.hints.ActualPrice.tooltip}" id="it10">
                        <f:validator binding="#{row.bindings.ActualPrice.validator}"/>
                    </af:inputText>
                    </af:column>
                    <af:column headerText="#{bindings.PurchaseOrderLinesView.hints.ActualTotal.label}"
                               sortProperty="#{bindings.PurchaseOrderLinesView.hints.ActualTotal.name}" sortable="false"
                               id="c13">
                        <af:outputText value="#{row.ActualTotal}" id="ot23"/>
                    </af:column>
                    <af:column headerText="#{bindings.PurchaseOrderLinesView.hints.ReceiveQuantity.label}"
                               sortProperty="#{bindings.PurchaseOrderLinesView.hints.ReceiveQuantity.name}"
                               sortable="false" id="c15">
                        <af:inputText value="#{row.bindings.ReceiveQuantity.inputValue}"
                                  label="#{bindings.PurchaseOrderLinesView.hints.ReceiveQuantity.label}"
                                  required="#{bindings.PurchaseOrderLinesView.hints.ReceiveQuantity.mandatory}"
                                  columns="#{bindings.PurchaseOrderLinesView.hints.ReceiveQuantity.displayWidth}"
                                  maximumLength="#{bindings.PurchaseOrderLinesView.hints.ReceiveQuantity.precision}"
                                  shortDesc="#{bindings.PurchaseOrderLinesView.hints.ReceiveQuantity.tooltip}"
                                  id="it13">
                            <!--<f:validator binding="#{row.bindings.ReceiveQuantity.validator}"/>-->
                        </af:inputText>
                    </af:column>
