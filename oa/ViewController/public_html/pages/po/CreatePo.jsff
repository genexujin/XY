                                      showRequired="false" autoSubmit="true">
                        <!--<f:validator binding="#{row.bindings.ItemDescription.validator}"/>-->
                    </af:inputText>
                    </af:column>
                    <af:column headerText="#{bindings.PurchaseOrderLinesView.hints.SubmitQuantity.label}"
                               sortProperty="#{bindings.PurchaseOrderLinesView.hints.SubmitQuantity.name}"
                               sortable="false" id="c4" width="60">
                        <af:inputText value="#{row.bindings.SubmitQuantity.inputValue}"
                                  label="#{bindings.PurchaseOrderLinesView.hints.SubmitQuantity.label}"
                                  required="#{bindings.PurchaseOrderLinesView.hints.SubmitQuantity.mandatory}"
                                  columns="#{bindings.PurchaseOrderLinesView.hints.SubmitQuantity.displayWidth}"
                                  maximumLength="#{bindings.PurchaseOrderLinesView.hints.SubmitQuantity.precision}"
                                  shortDesc="#{bindings.PurchaseOrderLinesView.hints.SubmitQuantity.tooltip}" id="it3"
                                  readOnly="#{pageFlowScope.fromMenu eq 'verifier' or pageFlowScope.fromMenu eq 'approver' or pageFlowScope.fromMenu eq 'receiver' 
                                              or (bindings.State.inputValue ne '1' and bindings.State.inputValue ne '4')}"
                                      autoSubmit="true">
                        <f:validator binding="#{row.bindings.SubmitQuantity.validator}"/>
                    </af:inputText>
