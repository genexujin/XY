    <attributeValues IterBinding="VehicleDMLIterator" id="ContactPhone">
      <AttrNames>
        <Item Value="ContactPhone"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="VehicleDMLIterator" id="State">
      <AttrNames>
        <Item Value="State"/>
