<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="11.1.2.62.76" id="RoleMenusPageDef"
                Package="pages.admin">
  <parameters/>
 <executables>
