  <SQLQuery><![CDATA[SELECT 
    ct.CONTRACT_NO CONTRACT_NO, 
    ct.PROJECT_NAME PROJECT_NAME, 
    ct.CONTRACT_ENTITY CONTRACT_ENTITY, 
    ct.TOTAL_AMOUNT TOTAL_AMOUNT, 
    ct.DESIGN_DEPT DESIGN_DEPT, 
    prjMgr.LASTNAME PROJECT_MANAGER, 
    regMgr.LASTNAME REGION_MANAGER, 
    paid.TOTALAMOUNT PaidAmount, 
    paid.NUMOFPAYMENT paidNo, 
    intact.TOTALAMOUNT IntactAmount, 
    intact.NUMOFPAYMENT IntactNo, 
    inv.TOTALAMOUNT InvoicedAmount, 
    inv.NUMOFPAYMENT InvoicedNo, 
    lastest.PAY1 PAY1, 
    lastest.PAY2 PAY2, 
    lastest.PAY3 PAY3, 
    lastest.PAY4 PAY4, 
    lastest.PAY5 PAY5, 
    lastest.PAY6 PAY6, 
    lastest.PAY7 PAY7, 
    lastest.PAY8 PAY8, 
    ct.AUTH_NAME AUTH_NAME 
FROM 
    CONTRACT ct, 
    USERS prjMgr, 
    USERS regMgr, 
    (SELECT * FROM V$INTACT_PAYMENT_YEAR WHERE YR = :yearBind) intact, 
    (SELECT * FROM V$INVOICED_PAYMENT_YEAR WHERE YR = :yearBind) inv, 
    (SELECT * FROM V$PAID_PAYMENT_YEAR WHERE YR = :yearBind) paid, 
    (SELECT * FROM V$LASTEST_PAYMENT WHERE YR = :yearBind) lastest 
WHERE 
    ct.PROJECT_MANAGER = prjMgr.USERNAME AND (((((ct.REGION_MANAGER = regMgr.USERNAME AND ct.DELETED != 'Y') AND ct.ID = intact.CONTRACTID(+)) AND ct.ID = inv.CONTRACTID(+)) AND ct.ID = paid.CONTRACTID(+)) AND ct.ID = lastest.CONTRACTID(+)) AND ct.STATUS != '合同终止' AND ct.STATUS != '合同暂停' 
ORDER BY 
    ct.CONTRACT_NO]]></SQLQuery>
