  <SQLQuery><![CDATA[SELECT 
    CONTRACT_LINE.ID ID, 
    CONTRACT_LINE.HEADER_ID HEADER_ID, 
    CONTRACT_LINE.LINE_NO LINE_NO, 
    CONTRACT_LINE.SHIP_MNFT_NO SHIP_MNFT_NO, 
    CONTRACT_LINE.SHIP_OWNER SHIP_OWNER, 
    CONTRACT_LINE.SHIP_BROKER SHIP_BROKER, 
    CONTRACT_LINE.SHIP_NAME SHIP_NAME, 
    CONTRACT_LINE.STATUS STATUS, 
    CONTRACT_LINE.PRICE PRICE, 
    CONTRACT_LINE.FG_COMPLETION_DATE FG_COMPLETION_DATE, 
    CONTRACT_LINE.SHIP_PLANT SHIP_PLANT, 
    CONTRACT_LINE_PAYMENTS.ID ID1, 
    CONTRACT_LINE_PAYMENTS.LINE_ID LINE_ID, 
    CONTRACT_LINE_PAYMENTS.PAYMENT_NAME PAYMENT_NAME, 
    CONTRACT_LINE_PAYMENTS.PAYMENT_PLAN_DATE PAYMENT_PLAN_DATE, 
    CONTRACT_LINE_PAYMENTS.PAYMENT_PLAN_AMOUNT PAYMENT_PLAN_AMOUNT, 
    CONTRACT_LINE_PAYMENTS.PAYMENT_PLAN_INVOICE_DATE PAYMENT_PLAN_INVOICE_DATE, 
    CONTRACT_LINE_PAYMENTS.STATUS STATUS1, 
    CONTRACT_LINE_PAYMENTS.INVOICE_DATE INVOICE_DATE, 
    CONTRACT_LINE_PAYMENTS.INVOICE_AMOUNT INVOICE_AMOUNT, 
    CONTRACT_LINE_PAYMENTS.TOTAL_PAYMENT TOTAL_PAYMENT, 
    CONTRACT_LINE_PAYMENTS.VARIANCE VARIANCE, 
    CONTRACT_LINE_PAYMENTS.MEMO MEMO1, 
    CONTRACT_LINE_PAYMENTS.INVOICE_NUMBER INVOICE_NUMBER, 
    CONTRACT_LINE_PAYMENTS.INVOICE_BATCH_ID INVOICE_BATCH_ID, 
    CONTRACT.ID ID2, 
    CONTRACT.CONTRACT_NO CONTRACT_NO, 
    CONTRACT.PROJECT_NAME PROJECT_NAME, 
    CONTRACT.CONTRACT_ENTITY CONTRACT_ENTITY, 
    CONTRACT.AUTH_TYPE AUTH_TYPE, 
    CONTRACT.AUTH_NAME AUTH_NAME, 
    CONTRACT.CUSTOMER_CONTACT_NAME CUSTOMER_CONTACT_NAME, 
    CONTRACT_LINE.WORK_START_DATE WORK_START_DATE, 
    ROUND(30 * MONTHS_BETWEEN(CONTRACT_LINE_PAYMENTS.PAYMENT_PLAN_INVOICE_DATE, SYSDATE)) day2Due, 
    CONTRACT.PROJECT_MANAGER PROJECT_MANAGER, 
    CONTRACT.REGION_MANAGER REGION_MANAGER, 
    CONTRACT.DESIGN_DEPT DESIGN_DEPT 
FROM 
    CONTRACT_LINE, 
    CONTRACT_LINE_PAYMENTS, 
    CONTRACT 
WHERE 
    CONTRACT_LINE_PAYMENTS.LINE_ID = CONTRACT_LINE.ID AND (((((CONTRACT_LINE.HEADER_ID = CONTRACT.ID AND CONTRACT_LINE.STATUS = :lineStatus) AND (CONTRACT_LINE_PAYMENTS.STATUS = :paymentStatus or CONTRACT_LINE_PAYMENTS.STATUS ='提交被退')) AND CONTRACT_LINE_PAYMENTS.DELETED != 'Y') AND CONTRACT_LINE.DELETED != 'Y') AND CONTRACT.DELETED != 'Y') AND ROUND(30 * MONTHS_BETWEEN(CONTRACT_LINE_PAYMENTS.PAYMENT_PLAN_INVOICE_DATE, SYSDATE)) <= :highDayLimit AND ROUND(30 * MONTHS_BETWEEN(CONTRACT_LINE_PAYMENTS.PAYMENT_PLAN_INVOICE_DATE, SYSDATE)) > :lowDayLimit]]></SQLQuery>
